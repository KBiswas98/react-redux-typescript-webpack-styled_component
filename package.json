{
  "name": "Aiofmes",
  "version": "1.0.0",
  "main": "dist/index.html",
  "description": "Created by aiofmes.com",
  "scripts": {
    "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node server.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "clean": "rm -rf .parcel-cache dist",
    "prepare": "husky install",
    "pm2": "npm run build:make && NODE_ENV=production pm2 start server.js",
    "build:start": "npm run build:make && NODE_ENV=production node server.js",
    "sk:dev": "node server.js",
    "build:make": "parcel build src/index.html"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@parcel/transformer-image": "^2.0.0-nightly.2358",
    "@parcel/transformer-postcss": "^2.0.0-alpha.3",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react": "17.0.8",
    "@types/react-date-range": "^1.1.6",
    "@types/react-dom": "17.0.5",
    "@types/react-select": "^4.0.17",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "10.2.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "module-alias": "^2.2.2",
    "parcel": "^2.0.0-nightly.548",
    "postcss": "8.3.0",
    "postcss-modules": "^4.1.3",
    "postcss-nested": "^5.0.6",
    "postcss-value-parser": "^4.1.0",
    "prettier": "^2.3.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.1.0",
    "sharp": "^0.28.3",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.15",
    "typescript": "4.3.2",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "gsap": "^3.7.1",
    "html-webpack-plugin": "^5.3.2",
    "image-webpack-loader": "^7.0.1",
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.1",
